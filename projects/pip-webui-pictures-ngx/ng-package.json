{
  "$schema": "../../node_modules/ng-packagr/ng-package.schema.json",
  "dest": "../../dist/pip-webui-pictures-ngx",
  "lib": {
    "entryFile": "src/public_api.ts"
  },
  "assets": [
    {
      "input": "assets",
      "glob": "**/*",
      "output": "assets"
    },
    {
      "input": "src",
      "glob": "**/*-theme.scss",
      "output": ""
    }
  ],
  "allowedNonPeerDependencies": ["ngx-webcam"]
}
